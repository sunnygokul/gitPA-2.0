PORT=3000
NODE_ENV=development

# GitHub API Configuration
# Get this from GitHub Developer Settings -> Personal Access Tokens
# Required permissions: repo (for public repos) or public_repo (for public only)
GITHUB_TOKEN=your_github_token_here

# Google Gemini API Configuration (FREE - 1500 requests/day)
# Get this from https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here 